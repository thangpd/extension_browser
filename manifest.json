{
  "manifest_version": 2,
  "name": "Reverse Zalo Messenger",
  "version": "2.1",
  "description": "Gets information from Google.",
  "icons": {
    "16": "assets/image/icon_128.png",
    "32": "assets/image/icon_128.png",
    "48": "assets/image/icon_128.png",
    "128": "assets/image/icon_128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "assets/js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "assets/js/lib/jquery-3.4.1.min.js",
        "assets/js/content.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "assets/image/icon_128.png"
  }
}